{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"lib": ["es6"],
		"types": ["reflect-metadata"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
